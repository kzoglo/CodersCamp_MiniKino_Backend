{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/index.ts",
      "sourceMaps": true,
      "trace": true,
      "outFiles": [
        "${workspaceFolder}/build/**/*.js"
      ],
      "env": {
        "NODE_ENV": "testing",
        "CINEMA_JWT_PRIVATE_KEY": "testValue"
      },
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "preLaunchTask": "prepare-app-to-debug",
      "postDebugTask": "clean-app-after-debug"
    }
  ]
}
